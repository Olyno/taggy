{
	"name": "taggy",
	"version": "1.0.0",
	"types": "module",
	"workspaces": [
		"apps/*"
	],
	"scripts": {
		"start": "yarn workspaces foreach -p run start",
		"start:bot": "yarn workspace @taggy/bot run start",
		"start:web": "yarn workspace @taggy/web run start",
		"start:websocket": "yarn workspace @taggy/websocket run start",
		"dev": "yarn workspaces foreach -p run dev",
		"dev:bot": "yarn workspace @taggy/bot run dev",
		"dev:web": "yarn workspace @taggy/web run dev",
		"dev:websocket": "yarn workspace @taggy/websocket run dev",
		"format": "yarn workspaces foreach -p run format",
		"format:bot": "yarn workspace @taggy/bot run format",
		"format:web": "yarn workspace @taggy/web run format",
		"format:websocket": "yarn workspace @taggy/websocket run format",
		"test": "node ./t.js"
	},
	"devDependencies": {
		"@types/debug": "^4.1.7",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"prisma": "^3.14.0",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^3.14.0",
		"debug": "^4.3.4",
		"dotenv": "^16.0.1",
		"envsafe": "^2.0.3"
	},
	"packageManager": "yarn@3.2.1"
}
